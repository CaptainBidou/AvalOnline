# Fichier de configuration Doxygen


# Nom du projet
PROJECT_NAME = "Avalonline"

# Répertoires à analyser
INPUT = src
INPUT += includes
# Sortie pour la documentation générée
OUTPUT_DIRECTORY = docs

# Extensions de fichiers à analyser
FILE_PATTERNS = *.h *.c

# Activer la génération de documentation pour les commentaires Doxygen
EXTRACT_ALL = YES

# Options de format de documentation
GENERATE_HTML = YES
GENERATE_LATEX = YES
